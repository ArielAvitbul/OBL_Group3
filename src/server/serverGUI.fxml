<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="mainPane" prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="server.serverGUI">
   <children>
      <VBox alignment="CENTER" layoutX="50.0" layoutY="12.0" prefHeight="150.0" prefWidth="100.0" spacing="15.0">
         <children>
            <Label text="Connect to server" />
            <TextField fx:id="sqlUserField" alignment="CENTER" promptText="SQL USER" text="root" />
            <PasswordField fx:id="sqlPassField" alignment="CENTER" promptText="SQL PASSWORD" text="arielsql" />
            <TextField fx:id="portField" alignment="CENTER" promptText="PORT" text="6001" />
            <Button mnemonicParsing="false" onMouseReleased="#launchServer" text="Launch Server" />
         </children>
      </VBox>
   </children>
</AnchorPane>
